{
  "name": "@gaming.tools/gtmap",
  "version": "0.2.4",
  "private": false,
  "license": "MIT",
  "author": "gaming.tools",
  "homepage": "https://gaming.tools",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/api/**",
    "dist/entities/**",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "clean": "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true})}catch(e){}\"",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:types": "node -e \"try{require('fs').rmSync('tsconfig.tsbuildinfo',{force:true})}catch(e){}\" && tsc -p ./tsconfig.types.json",
    "build:js": "esbuild src/index.ts --bundle --format=esm --platform=browser --minify --outfile=dist/index.js",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "prepack": "npm run build && node -e \"const fs=require('fs'),p=require('path');try{fs.copyFileSync(p.resolve(__dirname,'..','..','LICENSE'), p.resolve(__dirname,'LICENSE'))}catch(e){}\"",
    "pack": "npm pack"
  },
  "devDependencies": {
    "esbuild": "^0.25.9"
  },
  "sideEffects": false
}
